{
  "name": "bookmart",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "concurrently": "^9.1.2",
    "cra-template": "1.2.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-scripts": "5.0.1"
  },
  "scripts": {
